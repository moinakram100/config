<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
    <SelectDialog
        id="configProfileMap_DialogdocTypeLP"
        noDataText="Loading ..."
        initialFocus="SearchField"
        title="Select Document Type"
        liveChange=".onFilterDocTypeDialog"
        confirm="onValueHelpDialogSalesClose"
        cancel="onValueHelpDialogCancel"
           items="{
            path: '/xGMSxContractType',
            parameters : {
                $$operationMode : 'Server',
                $filter : 'Kopgr	 eq \'LP\''
              
            }
           
         
        }">
        <StandardListItem id="configProfileMap_StandardListItemLP"
            title="{Auart}"
            description="{Description}"  
            info="{Kopgr}"   
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>