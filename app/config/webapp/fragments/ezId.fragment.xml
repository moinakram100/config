<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
 
    <SelectDialog
        id="PenalityMapPenalityIdDialog"
        noDataText="No Items Found"
        title="Select Penalty ID"
        search="onValueHelpSearchEz"
        liveChange="onValueHelpSearchEz"
        confirm="onValueHelpCloseEz"
        cancel="onValueHelpCloseEz"
        items="{/EzIdTable}">
        <StandardListItem
            id="PenalityMapPenalityIdList"
            title="{EzID}"
            description="{EzIdDescription}"
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>