<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
 
    <SelectDialog
        id="PenalityMappingProfileParameterDialog"
        noDataText="No Parameter Found"
        title="Select Parameter"
        search="onValueHelpSearchPenaltyParam"
        liveChange="onValueHelpSearchPenaltyParam"
        confirm="onValueHelpClosePenalty"
        cancel="onValueHelpClosePenalty"
        items="{paneltyVal>/}">
        <StandardListItem id="PenalityMappingProfileParameterList"
            title="{paneltyVal>serviceParameter}"
            description="{paneltyVal>serviceParameterDesc}"
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>