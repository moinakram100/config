<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="PathFuelRedeliveryPointDialog"
        noDataText="Loading ..."
        title="Select Redelivery Location"
        search="onSearchRedeliveryLocation"
        liveChange="onSearchRedeliveryLocation"
        confirm="onRedeliverypointSelection"
        cancel="onRedeliverypointSelection"
       items="{
            path : '/xGMSxLocPoint_Map',
            parameters : {
                $$operationMode : 'Server',
                $filter : 'isRedeliveryPoint	 eq \'YES\''
              
            }
        }">
        <StandardListItem id="PathFuelRedeliveryPointList"
            title="{Locid}" 
              description="{Tsyst}"   
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>


