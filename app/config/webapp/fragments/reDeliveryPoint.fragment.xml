<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="docType34"
        noDataText="Loading ..."
        title="Select Location"
        search="onValueHelpDialogSearch"
        liveChange="onValueHelpDialogSearch"
        confirm="onValueredivery"
        cancel="onValueredivery"
       items="{
            path : '/xGMSxLocPoint_Map',
            parameters : {
                $$operationMode : 'Server',
                $filter : 'isRedeliveryPoint	 eq \'YES\''
              
            }
        }">
        <StandardListItem id="_IDGenStanddLtItem13"
            title="{Locid}" 
              description="{Tsyst}"   
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>


