<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
    <SelectDialog
        id="docTypeLP"
        noDataText="No Document Found"
        initialFocus="SearchField"
        title="Select Document Type"
        search="onValueHelpDialogSearch"
        confirm="onValueHelpDialogSalesClose"
        cancel="onValueHelpDialogCancel"
        items="{doctypeLPmodel>/}">
        <StandardListItem id="_IDGenStandardListItemLP"
            title="{doctypeLPmodel>Auart}"
            description="{doctypeLPmodel>Description}"  
            info="{doctypeLPmodel>Kopgr}"   
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>