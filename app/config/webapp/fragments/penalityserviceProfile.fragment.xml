
 <core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
       
        id="PenalityMapServiceProfileDialog"
        initialFocus="SearchField"
        noDataText="No Service Profile Found"
        title="Select Service Profile"
        search="onServiceProfileValueHelpDialogSearch"
        confirm="onServiceProfileValueHelpClose"
        cancel="onServiceProfileValueHelpCancel"
        liveChange="onServiceProfileValueHelpDialogSearch"
        items="{/ServiceProfileMaster}">
        <StandardListItem
            id="PenalityMapServiceProfileList"
            title="{serviceProfileName}"
            description="{serviceProfileDesc}"     
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>

