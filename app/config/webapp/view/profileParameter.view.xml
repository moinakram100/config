<mvc:View
    controllerName="com.ingenx.config.controller.profileParameter"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="profileParameterPage"
        showHeader="false"
    >
        <content>
            <Table
                items="{/serviceProfileParametersItems}"
                id="profileParameterTable"
                selectionChange="onSelect"
                mode="None"
                sticky="ColumnHeaders,HeaderToolbar"
            >
                <headerToolbar>
                    <OverflowToolbar
                        id="profileParameterOverFlow"
                        height="50px"
                    >
                        <content>
                            <Title id="profileParameter_profileName" text="{profileModel>/ProfileName} [{profileModel>/ProfileDesc}]"/>
                        </content>
                    </OverflowToolbar>
                </headerToolbar>

                <columns>
                    <Column
                        id="selectLabelCol"
                        width="6rem"
                    >
                        <header>
                            <Label
                                id="selectLabel"
                                text="Select"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_colID"
                        width="7rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_labelID"
                                wrapping="true"
                                text="ID"
                            />
                        </header>
                    </Column>

                    <Column
                        id="ProfileParam_ServiceParamCol"
                        width="7rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_ServiceParamLabel"
                                wrapping="true"
                                text="Service Parameter"
                            />
                        </header>
                    </Column>

                    <Column
                        id="ProfileParam_ContractRelCol"
                        width="7rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_ContractRelLabel"
                                wrapping="true"
                                text="Contract Relevant"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_valueParamCol"
                        width="7rem"
                    >
                        <header>
                            <Label
                                id="_IDGenLabel8"
                                wrapping="true"
                                text="ProfileParam_valueParamLabel"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_ThresholdRelevanceCol"
                        width="7rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_ThresholdRelevanceLabel"
                                wrapping="true"
                                text="Threshold Relevance"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_ThresholdRefCol"
                        width="8rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_ThresholdRefLabel"
                                wrapping="true"
                                text="Threshold Reference"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_nomRelevantCol"
                        width="8rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_nomRelevantLabel"
                                wrapping="true"
                                text="Nomination Relevant"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_BalancingRelCol"
                        width="8rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_BalancingRelLabel"
                                wrapping="true"
                                text="Balancing Relevant"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_allocRelCol"
                        width="8rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_allocRelLabel"
                                wrapping="true"
                                text="Allocation Relevant"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_billingRelCol"
                        width="6rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_billingRelLabel"
                                wrapping="true"
                                text="Billing Relevant"
                            />
                        </header>
                    </Column>
                    <Column
                        id="ProfileParam_penaltyRelCol"
                        width="6rem"
                    >
                        <header>
                            <Label
                                id="ProfileParam_penaltyRelLabel"
                                wrapping="true"
                                text="Penalty Relevant"
                            />
                        </header>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem
                        type="Active"
                        id="ProfileParam_listItem"
                    >
                        <cells>
                            <CheckBox
                                id="checkedParameter"
                                selected="{checkedParameter}"
                            />
                            <Text
                                id="ProfileParam_id2"
                                text="{ID}"
                            />
                            <Text
                                id="ProfileParam_servParam"
                                text="{serviceParameter}"
                            />
                            <!-- <Input id="id3" value="{ParentId}" width="30px"/> -->
                            <CheckBox
                                id="ContractRelevant"
                                selected="{ContractRelevant}"
                            />
                            <CheckBox
                                id="Value_Parameter"
                                selected="{Value_Parameter}"
                            />
                            <CheckBox
                                id="Threshold_Relevant"
                                selected="{Threshold_Relevance}"
                            />
                            <CheckBox
                                id="Referrence_Relevant"
                                selected="{Referrence_Relevant}"
                            />
                            <CheckBox
                                id="Nomination_Relevant"
                                selected="{Nomination_Relevant}"
                            />
                            <CheckBox
                                id="Balancing_Relevant"
                                selected="{Balancing_Relevant}"
                            />
                            <CheckBox
                                id="Allocation_Relevant"
                                select="onSelectAllocRel"
                                selected="{Allocation_Relevant}"
                            />
                            <CheckBox
                                id="Billing_Relevant"
                                selected="{Billing_Relevant}"
                            />
                            <CheckBox
                                id="Price_Relevant"
                                selected="{Price_Relevant}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
                <noData>
                    <IllustratedMessage
                        id="ProfileParam_IllustratedMessage"
                        enableVerticalResponsiveness="true"
                        title="No Data Found"
                        description="There are no items in this Table"
                    />
                </noData>
            </Table>
        </content>
    </Page>
</mvc:View>
